					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="UPC"/>
							<xsd:enumeration value="EAN"/>
							<xsd:enumeration value="GTIN"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Value">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="8"/>
							<xsd:maxLength value="16"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	
