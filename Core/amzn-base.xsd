	<!--
		
    ##################################################
    # Denomination Type
    ##################################################
    
	-->
	<xsd:element name="Denomination">
		<xsd:simpleType>
			<xsd:restriction base="xsd:decimal">
				<xsd:totalDigits value="6"/>
				<xsd:fractionDigits value="2"/>
				<xsd:minInclusive value=".01"/>
				<xsd:maxInclusive value="2500.00"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:element>
	<!--
		
    ##################################################
    # Integer Types
    ##################################################
    
	-->
